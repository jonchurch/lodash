{
  "name": "lodash",
  "version": "5.0.0",
  "license": "MIT",
  "private": true,
  "main": "lodash.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "sideEffects": false,
  "scripts": {
    "lint": "eslint *.js .internal/**/*.js test/**/*.js",
    "test": "mocha -r esm test/*.test.js",
    "test:bail": "mocha --bail -r esm test/*.test.js",
    "test:coverage": "nyc npm test",
    "validate": "npm run style && npm run test"
  },
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-mocha": "^10.1.0",
    "esm": "^3.2.25",
    "lodash": "4.17.20",
    "mocha": "^5.2.0",
    "nyc": "^15.1.0"
  }
}
